<h6>Follow me</h6>
<ul class="social-media">
    <% if (theme.social.github){ %><li><a title="{{ site.social.github }} on Github" href="https://github.com/{{ site.social.github }}" target="_blank"><i class="fa fa-github fa-2x"></i></a></li><% } %>
    <% if (theme.social.stackoverflow){ %><li><a title="{{ site.social.stackoverflow }} on StackOverflow" href="http://stackoverflow.com/users/1252056/{{ site.social.stackoverflow }}" target="_blank"><i class="fa fa-stack-overflow fa-2x"></i></a></li><% } %>
    <% if (theme.social.linkedin){ %><li><a title="{{ site.social.github }} on LinkedIn" href="https://www.linkedin.com/in/{{ site.social.linkedin }}" target="_blank"><i class="fa fa-linkedin fa-2x"></i></a></li><% } %>
    <% if (theme.social.instagram){ %><li><a title="{{ site.social.instagram }} on Instagram" href="https://instagram.com/{{ site.social.instagram }}" target="_blank"><i class="fa fa-instagram fa-2x"></i></a></li><% } %>
    <% if (theme.social.lastfm){ %><li><a title="{{ site.social.lastfm }} on LinkedIn" href="http://lastfm.com/user/{{ site.social.lastfm }}" target="_blank"><i class="fa fa-lastfm fa-2x"></i></a></li><% } %>
    <% if (theme.social.rss){ %><li><a title="{{ site.social.rss }} RSS" href="{{site.url}}/{{ site.social.rss }}" target="_blank"><i class="fa fa-rss fa-2x"></i></a></li><% } %>
</ul>
